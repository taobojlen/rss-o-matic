    "drizzle-orm": "^0.45.1",
    "nanoid": "^5.1.6",
    "nuxt": "^4.3.1",
    "posthog-js": "^1.352.1",
    "posthog-node": "^5.24.17",
    "unstorage": "^1.17.4",
    "vue": "^3.5.28",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@posthog/cli": "^0.6.1",
    "typescript": "^5.9.3",
    "wrangler": "^4.67.0"
  }
