{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "rss-o-matic",
  "compatibility_date": "2025-02-24",
  "observability": {
    "logs": {
      "enabled": true,
      "invocation_logs": true
    }
  },
  "vars": {
    "NUXT_OPENROUTER_MODEL": "minimax/minimax-m2.5"
  },
"kv_namespaces": [
    {
      "binding": "KV",
      "id": "405d94875ef249ff866a5314e032ff8f"
    }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "rss-o-matic",
      "database_id": "31ed3c46-030e-4448-b4ad-fe38e055cedf",
      "migrations_table": "_hub_migrations",
      "migrations_dir": "server/db/migrations"
    }
  ],
  "env": {
    "preview": {
      "name": "rss-o-matic-preview",
      "kv_namespaces": [
        {
          "binding": "KV",
          "id": "f63d2ce7303846acb066a2e1c8022e09"
        }
      ],
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "rss-o-matic-preview",
          "database_id": "007b6207-07d6-4fac-88fb-29714dd52d65",
          "migrations_table": "_hub_migrations",
          "migrations_dir": "server/db/migrations"
        }
      ]
    }
  }
}
